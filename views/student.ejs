<!DOCTYPE html>
<html>
<head>
  <title>STUDENTS</title>
  <style>
    table{width:500px;border:1px solid #333;}
    table th{padding:5px;background: #ccc; text-align: left;}
    table td{padding:5px;}
  </style>
</head>
<body>
  <h1>STUDENTS</h1>
  <a href="/students/add">ADD</a>
  <table border="0" cellpadding="0" cellspacing="0">
    <tr>
      <th>ID</th>
      <th>FIRST NAME</th>
      <th>LAST NAME</th>
      <th>EMAIL</th>
      <th>Action</th>
    </tr>

    <% students.forEach (data =>{%>
      <tr>
        <td>&nbsp<%= data.id %></td>
        <td>&nbsp<%= data.first_name %></td>
        <td>&nbsp<%= data.last_name %></td>
        <td>&nbsp<%= data.email %></td>
        <td><a href="/students/edit/<%= data.id %>">Edit</a>|<a href="/students/delete/<%= data.id %>">Delete</a></td>
      </tr>
      <% }) %>
  </table>

</body>
</html>
