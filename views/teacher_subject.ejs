<!DOCTYPE html>
<html>
<head>
  <title>Teacher</title>
  <style>
    table{width:500px;border:1px solid #333;}
    table th{padding:5px;background: #ccc; text-align: left;}
    table td{padding:5px;}
  </style>
</head>
<body>
  <h1>SUBJECTS TEACHER</h1>
  <a href="/">HOME</a>|
  <table border="0" cellpadding="0" cellspacing="0">
    <tr>
      <th>ID</th>
      <th>SUBJECT NAME</th>
      <th>Teacher</th>
    </tr>

    <% subjects.forEach (data =>{%>
      <tr>
        <td>&nbsp<%= data.id %></td>
        <td>&nbsp<%= data.subject_name %></td>
        <td>
            <% data.Teachers.forEach (dataTeachers =>{%>
                <%= dataTeachers.first_name+" "+dataTeachers.last_name %><br>
          <%})%>
        </td>
      </tr>
      <% }) %>

  </table>
</body>
</html>
