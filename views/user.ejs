<!DOCTYPE html>
<html>
<head>
  <title>STUDENTS</title>
  <style>
    table{width:500px;border:1px solid #333;}
    table th{padding:5px;background: #ccc; text-align: left;}
    table td{padding:5px;}
  </style>
</head>
<body>
  <h1>STUDENTS</h1>
  <a href="/">HOME</a>|<a href="/users/add">ADD</a>
  <table border="0" cellpadding="0" cellspacing="0">
    <tr>
      <th>ID</th>
      <th>User Name</th>
      <th>ROOL</th>
      <th>Option</th>
    </tr>

    <% users.forEach (data =>{%>
      <tr>
        <td>&nbsp<%= data.id %></td>
        <td>&nbsp<%= data.user_id %></td>
        <td>&nbsp<%= data.rool %></td>
        <td><a href="/users/edit/<%= data.id %>">Edit</a>|<a href="/users/delete/<%=data.id %>">Delete</a></td>
      </tr>
      <% }) %>
  </table>

</body>
</html>
