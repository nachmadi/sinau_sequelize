<!DOCTYPE html>
<html>
<head>
  <title>SUBJECTS</title>
  <style>
    table{width:500px;border:1px solid #333;}
    table th{padding:5px;background: #ccc; text-align: left;}
    table td{padding:5px;}
  </style>
</head>
<body>
  <h1>SUBJECTS</h1>
  <table border="0" cellpadding="0" cellspacing="0">
    <tr>
      <th>ID</th>
      <th>SUBJECT NAME</th>
      <th>Action</th>
    </tr>

    <% subjects.forEach (data =>{%>
      <tr>
        <td>&nbsp<%= data.id %></td>
        <td>&nbsp<%= data.subject_name %></td>
        <td><a href="/subjects/edit/<%= data.id %>">Edit</a>|<a href="/subjects/delete/<%=data.id %>">Delete</a></td>
      </tr>
      <% }) %>

  </table>
</body>
</html>
